{
	"name" : "jms/cg",
	"description" : "Toolset for generating PHP code",
	"keywords" : [
		"code generation"
	],
	"type" : "library",
	"license" : "Apache2",
	"authors" : [{
			"name" : "Johannes M. Schmitt",
			"email" : "schmittjoh@gmail.com"
		}, {
			"name" : "Thomas Gossmann",
			"homepage" : "http://gos.si"
		}
	],
	"require" : {
		"php" : ">=5.4.0",
		"doctrine/annotations" : "~1",
		"phpdocumentor/reflection-docblock" : "~2"
	},
	"autoload" : {
		"psr-0" : {
			"CG" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"GC\\Tests\\" : "tests/CG/Tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.1-dev"
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4",
		"sami/sami" : "~1"
	}
}